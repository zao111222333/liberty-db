{
    // "window.zoomLevel": 1,
    "[rust]": {
        "editor.defaultFormatter": "rust-lang.rust-analyzer",
        "editor.formatOnSave": true,
    },
    // "rust-analyzer.checkOnSave": true,
    // "rust-analyzer.check.command": "clippy",
    // "rust-analyzer.check.extraArgs": [], // "--all-features"
    "rust-analyzer.server.path": "rust-analyzer",
    "rust-analyzer.procMacro.enable": true,
    "rust-analyzer.showUnlinkedFileNotification": false,
    // "rust-analyzer.cargo.features": ["py"],
    // "rust-analyzer.cargo.features": ["tracing"],
    "todo-tree.general.tags":[
        "BUG",
        "HACK",
        "NOTICE",
        "FIXME",
        "TODO",
    ],
    "todo-tree.highlights.customHighlight": {
        "TODO": {
            "type": "text",
            "borderRadius":"0",
            "foreground": "#ffffff",
            "background": "#ffbd2a",
            "rulerColour":"#ffbd2a",
            "rulerLane":"left",
            "icon": "checkbox",
            "iconColour": "#ffbd2a",
            "gutterIcon": true,
        },
        "FIXME": {
            "type": "text",
            "borderRadius":"0",
            "foreground": "#ffffff",
            "background": "#f06292",
            "rulerColour": "#ed6a5e",
            "rulerLane":"left",
            "icon": "alert",
            "iconColour": "#ed6a5e",
            "gutterIcon": true,
        },
        "NOTICE": {
            "type": "text",
            "borderRadius":"0",
            "foreground": "#ffffff",
            "background": "#3282e2",
            "rulerColour": "#3282e2",
            "rulerLane":"left",
            "icon": "note",
            "iconColour": "#3282e2",
            "gutterIcon": true,
        },
    },
    "files.associations": {
        "*.wasm": "wasm",
        "*.cdl": "spice",
        "si2dr_liberty.h": "c",
        "iostream": "cpp",
        "new": "cpp",
        "iosfwd": "cpp"
    }
}