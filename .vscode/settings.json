{
    "window.zoomLevel": 1,
    "[rust]": {
        "editor.defaultFormatter": "rust-lang.rust-analyzer",
        "editor.formatOnSave": true,
    },
    // "rust-analyzer.checkOnSave": true,
    // "rust-analyzer.check.command": "clippy",
    // "rust-analyzer.check.extraArgs": ["--all-features"],
    "rust-analyzer.procMacro.enable": true,
    // vscode-lldb need glibc 2.28
    "rust-analyzer.debug.engine": "vadimcn.vscode-lldb",
    "lldb.launch.initCommands": ["settings set target.disable-aslr false"],
    // "rust-analyzer.debug.engine": "ms-vscode.cpptools",
    // "rust-analyzer.debug.engineSettings": {
    //     "cppdbg": {            
    //         "miDebuggerPath": "${env:HOME}/.cargo/bin/rust-gdb",
    //         "setupCommands": [
    //             {
    //                 "description": "Enable pretty-printing for gdb",
    //                 "text": "-enable-pretty-printing",
    //                 "ignoreFailures": false
    //             }
    //         ],
    //     }
    // },
    "rust-analyzer.server.extraEnv": { 
        "RUSTUP_TOOLCHAIN": "stable",
        "RUSTUP_DIST_SERVER":"https://rsproxy.cn",
        "RUSTUP_UPDATE_ROOT":"https://rsproxy.cn/rustup"
    },
    "terminal.integrated.env.linux": {
        "RUSTUP_TOOLCHAIN": "stable",
        "RUSTUP_DIST_SERVER":"https://rsproxy.cn",
        "RUSTUP_UPDATE_ROOT":"https://rsproxy.cn/rustup"
    },
    "terminal.integrated.env.osx": {
        "RUSTUP_TOOLCHAIN": "stable",
        "RUSTUP_DIST_SERVER":"https://rsproxy.cn",
        "RUSTUP_UPDATE_ROOT":"https://rsproxy.cn/rustup"
    },
    "terminal.integrated.copyOnSelection": true,
    "todo-tree.general.tags":[
        "BUG",
        "HACK",
        "NOTICE",
        "FIXME",
        "TODO",
    ],
    "todo-tree.highlights.customHighlight": {
        "TODO": {
            "type": "text",
            "borderRadius":"0",
            "foreground": "#ffffff",
            "background": "#ffbd2a",
            "rulerColour":"#ffbd2a",
            "rulerLane":"left",
            "icon": "checkbox",
            "iconColour": "#ffbd2a",
            "gutterIcon": true,
        },
        "FIXME": {
            "type": "text",
            "borderRadius":"0",
            "foreground": "#ffffff",
            "background": "#f06292",
            "rulerColour": "#ed6a5e",
            "rulerLane":"left",
            "icon": "alert",
            "iconColour": "#ed6a5e",
            "gutterIcon": true,
        },
        "NOTICE": {
            "type": "text",
            "borderRadius":"0",
            "foreground": "#ffffff",
            "background": "#3282e2",
            "rulerColour": "#3282e2",
            "rulerLane":"left",
            "icon": "note",
            "iconColour": "#3282e2",
            "gutterIcon": true,
        },
    },
    "rust-analyzer.showUnlinkedFileNotification": false,
}